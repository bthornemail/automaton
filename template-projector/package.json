{
  "name": "canvasl-semantic-slides-projector",
  "version": "0.1.0",
  "description": "Browser-native semantic slides system with CanvasL, Meta-Log, and DBpedia integration",
  "type": "module",
  "main": "src/projector/Projector.js",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "test:jest": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:e2e": "vite preview --open test/e2e-test.html",
    "test:dbpedia": "vite preview --open test/dbpedia-test.html",
    "test:cors": "vite preview --open test/cors-test.html",
    "test:recovery": "vite preview --open test/error-recovery-test.html",
    "test:federation": "vite preview --open test/federation-test.html",
    "test:federation-verify": "vite preview --open test/federation-verification.html",
    "test:agent-protection": "vite preview --open test/agent-protection-browser-test.html",
    "test:performance": "vite preview --open test/performance-test.html",
    "test:verify": "node scripts/verify-test-infrastructure.js",
    "test:playwright": "playwright test",
    "test:playwright:headless": "playwright test --project=chromium",
    "test:playwright:init": "playwright test tests/init.spec.js",
    "test:playwright:e2e": "playwright test tests/e2e.spec.js",
    "test:playwright:example": "playwright test tests/example.spec.js",
    "test:playwright:setup": "playwright test tests/setup.spec.js",
    "test:playwright:ui": "playwright test --ui",
    "test:playwright:headed": "playwright test --headed",
    "test:playwright:debug": "playwright test --debug",
    "test:playwright:report": "playwright show-report",
    "test:playwright:install": "playwright install chromium",
    "test:playwright:install-deps": "playwright install --with-deps chromium",
    "test:playwright:verify": "node scripts/verify-playwright-setup.js"
  },
  "keywords": [
    "canvasl",
    "semantic-web",
    "presentations",
    "rdf",
    "sparql",
    "dbpedia",
    "meta-log"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "biwascheme": "^0.7.0",
    "n3": "^1.20.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "jest": "^29.7.0",
    "vite": "^5.0.0"
  }
}
